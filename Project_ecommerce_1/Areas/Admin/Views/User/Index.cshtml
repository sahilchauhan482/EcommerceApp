<div class="row">
    <div class="col-9">
        <h2 class="text-info">User List</h2>
    </div>
    <div class="col-3">
        <a asp-page="/Account/Register" asp-area ="Identity"class=" btn btn-primary form-control">
            <i class="fas fa-plus"></i> &nbsp;Add New User
        </a>
    </div>
</div>
<div class="container p-2 m-2 rounded border">
    <table id="tblData" class="table table-bordered table-hover ">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Company</th>
                <th>Role</th>
                <th class="text-center">Actions</th>
               
            </tr>
        </thead>

    </table>
</div>
@section scripts
    {
    <script src="~/js/User.js"></script>
}